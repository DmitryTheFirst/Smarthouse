<?xml version="1.0" encoding="utf-8" ?>
<config>
  <moduleManager>
    <listener port="2222"/>
    <client timeoutSecs="2"/>
    <smarthouses>
      <smarthouse ip="192.168.0.100" port="2222"/>
    </smarthouses>
  </moduleManager>
  <plugins>
    <plugin className="Smarthouse.TcpNetwork">
      <description>
        <desc name="name" value="NetworkMain"/>
      </description>
      <moduleConfig>
        <host localID="NetworkMain"/>
        <listener port="111"/>
        <sender sendTimer="100"/>
      </moduleConfig>
    </plugin>
    <plugin className="Smarthouse.TcpNetwork">
      <description>
        <desc name="name" value="NetworkAdditional1"/>
      </description>
      <moduleConfig>
        <host localID="NetworkAdditional1"/>
        <listener port="112"/>
        <sender sendTimer="100"/>
      </moduleConfig>
    </plugin>
    <plugin className="Smarthouse.TcpNetwork">
      <description>
        <desc name="name" value="NetworkAdditional2"/>
      </description>
      <moduleConfig>
        <host localID="NetworkAdditional2"/>
        <listener port="113"/>
        <sender sendTimer="100"/>
      </moduleConfig>
    </plugin>
    <plugin className="Smarthouse.Test">
      <description>
        <desc name="name" value="Test"/>
      </description>
      <moduleConfig>
        <netCfg ip="192.168.0.6"/>
      </moduleConfig>
    </plugin>
  </plugins>
</config>